{
    "configurations": [
        {
            "name": "STM32 H7",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32H743xx",
                "DCMI"
            ],
            "windowsSdkVersion": "10.0.19041.0",
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m7",
                "-mthumb"
            ]

        }
    ],
    "version": 4
}